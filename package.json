{
  "name": "tosho-in-wariate-kun",
  "version": "1.0.0",
  "description": "Digital library committee member scheduling system for Japanese elementary schools",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:unit": "turbo run test:unit",
    "test:integration": "turbo run test:integration",
    "test:e2e": "turbo run test:e2e",
    "test:coverage": "turbo run test:coverage",
    "test:report": "turbo run test:report",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,yaml}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build && changeset publish"
  },
  "keywords": [
    "education",
    "school",
    "library",
    "scheduling",
    "japanese",
    "elementary-school",
    "committee",
    "monorepo",
    "nextjs",
    "nestjs",
    "typescript"
  ],
  "author": "Tosho-in Wariate-kun Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/tosho-in-wariate-kun.git"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "tools/*"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/node": "^20.0.0",
    "prettier": "^3.0.0",
    "turbo": "^1.10.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}